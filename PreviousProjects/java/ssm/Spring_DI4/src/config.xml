<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean   id="helloworld"    class="org.beanpost.HelloWorld"  init-method="init">
    <property   name="message"><value>helloworld</value></property>
    </bean>
    <bean    class="org.beanpost.MyBeanPost"></bean>
    <bean    id="beanfactorypost"    class="org.beanpost.MyBeanFactoryPostProcessor"></bean>
</beans>